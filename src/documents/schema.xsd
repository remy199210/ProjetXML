            <xs:element name="details"/>
        </xs:sequence>
        <xs:attribute name="titre" type="xs:string"/>
    </xs:complexType>
    <xs:complexType name="CV">
        <xs:sequence>
            <xs:element name="etat_civil" type="tns:etat_civil"/>
            <xs:element name="coordonnees" type="tns:coordonnees"/>
            <xs:element name="permis">
                <xs:complexType>
                    <xs:attribute name="type" type="xs:string"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="formations">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="formation" type="tns:formation" maxOccurs="unbounded"/>
                    </xs:sequence>
                    <xs:attribute name="titre" type="xs:string" default="Parcours Universitaire"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="experiences">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="experience" maxOccurs="unbounded" type="tns:experience"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="competences">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="competence" type="tns:divers" maxOccurs="unbounded" minOccurs="0"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="interets">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="interet" maxOccurs="unbounded" type="tns:divers"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
